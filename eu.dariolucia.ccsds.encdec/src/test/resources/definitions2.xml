<ns1:packet_definitions xmlns:ns1="http://dariolucia.eu/ccsds/encdec">
    <packets>
        <packet id="DEF1">
            <structure>
                <!-- Signed integer 3 bits: 3 -->
                <parameter id="PARAM1">
                    <type_fixed type="SignedInteger" length="3" />
                </parameter>
                <!-- Float 32 bits: 35 -->
                <parameter id="PARAM2">
                    <type_fixed type="Real" length="1" />
                </parameter>
                <!-- Unsigned integer 6 bits: 41 -->
                <parameter id="PARAM3">
                    <type_fixed type="UnsignedInteger" length="6" />
                </parameter>
                <!-- Boolean 1 bit: 42 -->
                <parameter id="PARAM4">
                    <type_fixed type="Boolean" length="0" />
                </parameter>
                <!-- Boolean 1 bit: 43 -->
                <parameter id="PARAM5">
                    <type_fixed type="Boolean" length="0" />
                </parameter>
                <!-- Bitstring 13 bits: 56 -->
                <parameter id="PARAM6">
                    <type_fixed type="BitString" length="13" />
                </parameter>
                <!-- Octet string 3 bytes: 80 -->
                <parameter id="PARAM7">
                    <type_fixed type="OctetString" length="3" />
                </parameter>
                <!-- Char string 7 bytes: 136 -->
                <parameter id="PARAM8">
                    <type_fixed type="CharacterString" length="7" />
                </parameter>
                <!-- Boolean 1 bit: 137 -->
                <parameter id="PARAM9">
                    <type_fixed type="Boolean" length="0" />
                </parameter>
                <!-- Absolute time 6 bytes: 185 -->
                <parameter id="PARAM10">
                    <type_fixed type="AbsoluteTime" length="1" />
                </parameter>
                <!-- Relative time 2 bytes: 201 -->
                <parameter id="PARAM11">
                    <type_fixed type="RelativeTime" length="2" />
                </parameter>
                <!-- Signed integer 3 bits: 204 -->
                <parameter id="PARAM12">
                    <type_fixed type="UnsignedInteger" length="3" />
                </parameter>
            </structure>
        </packet>

        <packet id="DEF2">
            <structure>
                <!-- Signed integer 2 bits: 2 -->
                <parameter id="PARAM1">
                    <type_fixed type="SignedInteger" length="2" />
                </parameter>
                <!-- Enumeration 3 bits: 5 -->
                <parameter id="PARAM2">
                    <type_fixed type="Enumerated" length="3" />
                </parameter>
                <!-- Unsigned integer 3 bits: 8 -->
                <parameter id="PARAM3">
                    <type_fixed type="UnsignedInteger" length="3" />
                </parameter>
                <!-- Double 64 bits: 72 -->
                <parameter id="PARAM4">
                    <type_fixed type="Real" length="2" />
                </parameter>
                <!-- MIL 32 bits: 104 -->
                <parameter id="PARAM5">
                    <type_fixed type="Real" length="3" />
                </parameter>
                <!-- MIL 48 bits: 152 -->
                <parameter id="PARAM6">
                    <type_fixed type="Real" length="4" />
                </parameter>
                <!-- Absolute time CDS millis 8 bytes: 216 -->
                <parameter id="PARAM7">
                    <type_fixed type="AbsoluteTime" length="2" />
                </parameter>
                <!-- Absolute time CUC coarse 4, fine 3, 6 bytes: 272 -->
                <parameter id="PARAM8">
                    <type_fixed type="AbsoluteTime" length="18" />
                </parameter>
            </structure>
        </packet>

        <packet id="DEF3">
            <structure>
                <!-- Signed integer 3 bits: 3 -->
                <parameter id="PARAM1">
                    <type_fixed type="SignedInteger" length="3" />
                </parameter>
                <!-- Array 3 x 11 bits: 33 bits: 36 -->
                <array id="ARRAY1">
                    <size_fixed len="3" />
                    <parameter id="PARAM_A1">
                        <type_fixed type="SignedInteger" length="5" />
                    </parameter>
                    <parameter id="PARAM_A2">
                        <type_fixed type="SignedInteger" length="5" />
                    </parameter>
                    <parameter id="PARAM_A3">
                        <type_fixed type="Boolean" length="0" />
                    </parameter>
                </array>
                <!-- Signed integer 4 bits: 40 -->
                <parameter id="PARAM2">
                    <type_fixed type="SignedInteger" length="4" />
                </parameter>
            </structure>
        </packet>

        <packet id="DEF4">
            <structure>
                <!-- Signed integer 3 bits: 3 -->
                <parameter id="PARAM1">
                    <type_fixed type="SignedInteger" length="3" />
                </parameter>
                <!-- Structure 11 bits: 14 -->
                <structure id="STRUCT1">
                    <parameter id="PARAM_A1">
                        <type_fixed type="SignedInteger" length="5" />
                    </parameter>
                    <parameter id="PARAM_A2">
                        <type_fixed type="SignedInteger" length="5" />
                    </parameter>
                    <parameter id="PARAM_A3">
                        <type_fixed type="Boolean" length="0" />
                    </parameter>
                </structure>
                <!-- Signed integer 2 bits: 16 -->
                <parameter id="PARAM2">
                    <type_fixed type="SignedInteger" length="2" />
                </parameter>
            </structure>
        </packet>

        <packet id="DEF5">
            <structure>
                <!-- Signed integer 5 bits: 5 -->
                <parameter id="PARAM1">
                    <type_fixed type="SignedInteger" length="5" />
                </parameter>
                <!--  -->
                <array id="ARRAY1">
                    <size_fixed len="2" />
                    <parameter id="PARAM_A1">
                        <type_fixed type="SignedInteger" length="5" />
                    </parameter>
                    <array id="ARRAY2">
                        <size_ref ref="PARAM_A1" />
                        <parameter id="PARAM_AA1">
                            <type_fixed type="SignedInteger" length="5" />
                        </parameter>
                        <parameter id="PARAM_AA2">
                            <type_fixed type="SignedInteger" length="5" />
                        </parameter>
                    </array>
                </array>
                <!-- Signed integer 5 bits -->
                <parameter id="PARAM2">
                    <type_fixed type="SignedInteger" length="5" />
                </parameter>
                <parameter id="PARAM3">
                    <type_fixed type="Boolean" length="0" />
                </parameter>
                <parameter id="PARAM4">
                    <type_fixed type="Boolean" length="0" />
                </parameter>
            </structure>
        </packet>

        <packet id="DEF6">
            <structure>
                <array id="ARRAY1">
                    <size_fixed len="2" />
                    <structure id="STRUCT1">
                        <array id="ARRAY2">
                            <size_fixed len="2" />
                            <parameter id="PARAM_AA1">
                                <type_fixed type="SignedInteger" length="5" />
                            </parameter>
                        </array>
                        <structure id="STRUCT2">
                            <parameter id="PARAM_S2">
                                <type_fixed type="SignedInteger" length="6" />
                            </parameter>
                        </structure>
                    </structure>
                </array>
            </structure>
        </packet>

        <packet id="DEF7">
            <structure>
                <!-- Used as type for the next parameter: char string (8) -->
                <parameter id="PARAM1">
                    <type_fixed type="SignedInteger" length="5" />
                </parameter>
                <!-- Unsigned integer 6 bits: 11 -->
                <parameter id="PARAM2">
                    <type_fixed type="UnsignedInteger" length="8" />
                    <length_fixed len="6" />
                </parameter>
                <!-- String: 59 octets -->
                <parameter id="PARAM3">
                    <type_ref ref="PARAM1" />
                    <length_ref ref="PARAM2" />
                </parameter>
            </structure>
        </packet>

        <packet id="DEF8">
            <structure>
                <!-- Signed integer 5 bits -->
                <parameter id="PARAM1">
                    <type_fixed type="SignedInteger" length="5" />
                </parameter>
                <!-- Unsigned integer x bits: length overridden-->
                <parameter id="PARAM2">
                    <type_fixed type="UnsignedInteger" length="12" />
                    <length_ref ref="PARAM1" />
                </parameter>
            </structure>
        </packet>

        <packet id="DEF9">
            <structure>
                <!-- Signed integer 5 bits -->
                <parameter id="PARAM1">
                    <type_fixed type="SignedInteger" length="5" />
                </parameter>
                <!-- -->
                <parameter id="PARAM2">
                    <location_absolute bit_absolute="11" />
                    <type_fixed type="UnsignedInteger" length="12" />
                </parameter>
                <parameter id="PARAM3">
                    <location_last bit_offset="1" />
                    <type_fixed type="UnsignedInteger" length="6" />
                </parameter>
                <!-- this wil start at 32 -->
                <parameter id="PARAM4">
                    <location_last bit_align="8" />
                    <type_fixed type="UnsignedInteger" length="8" />
                </parameter>
                <parameter id="PARAM5">
                    <location_param ref="PARAM1" bit_offset="1" bit_align="6" />
                    <type_fixed type="UnsignedInteger" length="4" />
                </parameter>
            </structure>
        </packet>

        <packet id="DEF10">
            <structure>
                <!-- Signed integer 5 bits: 5 -->
                <parameter id="PARAM1">
                    <type_fixed type="SignedInteger" length="5" />
                </parameter>
                <!--  -->
                <array id="ARRAY1">
                    <size_ref ref="PARAM1" />
                    <!-- parameter id -->
                    <parameter id="PARAM_A1">
                        <type_fixed type="UnsignedInteger" length="12" />
                    </parameter>
                    <!-- value -->
                    <parameter id="PARAM_A2">
                        <type_param ref="PARAM_A1" />
                    </parameter>
                    <!-- another value -->
                    <parameter id="PARAM_A3">
                        <type_param ref="PARAM_A1" />
                    </parameter>
                </array>
            </structure>
        </packet>

        <packet id="DEF11">
            <structure>
                <!-- Absolute time 7 bytes + P field: 64 -->
                <parameter id="PARAM1">
                    <type_fixed type="AbsoluteTime" length="0" />
                </parameter>
                <!-- Relative time 7 bytes + P field: 128 -->
                <parameter id="PARAM2">
                    <type_fixed type="RelativeTime" length="0" />
                </parameter>
            </structure>
        </packet>

        <packet id="DEF12">
            <structure>
                <!-- Signed integer 3 bits: 8 -->
                <parameter id="PARAM1" pad_to="8">
                    <type_fixed type="SignedInteger" length="3" />
                </parameter>
                <!-- Float 32 bits: 48 -->
                <parameter id="PARAM2" pad_to="40">
                    <type_fixed type="Real" length="1" />
                </parameter>
                <!-- Boolean 1 bit: 50 -->
                <parameter id="PARAM3" pad_to="2">
                    <type_fixed type="Boolean" length="0" />
                </parameter>
                <!-- Boolean 1 bit: 56 -->
                <parameter id="PARAM4">
                    <type_fixed type="SignedInteger" length="6" />
                </parameter>
            </structure>
        </packet>
    </packets>

    <parameters>
        <parameter id="PARAM001" external_id="1234" description="Sample Parameter 1">
            <type type="UnsignedInteger" length="7" />
        </parameter>
        <parameter id="PARAM002" external_id="1235" description="Sample Parameter 2">
            <type type="SignedInteger" length="3" />
        </parameter>
        <parameter id="PARAM002" external_id="1236" description="Sample Parameter 3">
            <type type="Real" length="1" />
        </parameter>
    </parameters>

</ns1:packet_definitions>